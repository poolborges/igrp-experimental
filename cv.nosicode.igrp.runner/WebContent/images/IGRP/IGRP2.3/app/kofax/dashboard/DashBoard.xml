<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="images/IGRP/IGRP2.3/app/kofax/dashboard/DashBoard.xsl" type="text/xsl"?>
<rows>
    <site>
        <welcome_note>Ola</welcome_note>
        <footer_note>2011 - Copyright NOSI</footer_note>
        <user_name>red-igrp</user_name>
        <button type="specific">
            <title>Sair</title>
            <app>RED</app>
            <page />
            <link>#</link>
            <target>_self</target>
            <img>exit.png</img>
        </button>
    </site>
    <page>DashBoard</page>
    <app>kofax</app>
    <plsql>
        <action>index</action>
        <package_instance></package_instance>
        <package_db>nosi.webapps.kofax.pages</package_db>
        <package_html>DashBoard</package_html>
        <table_name></table_name>
        <with_replace>false</with_replace>
        <with_label>false</with_label>
        <with_biztalk>false</with_biztalk>
        <dynamic_menu>false</dynamic_menu>
        <copy_menu>false</copy_menu>
        <package_copy_db />
        <package_copy_html />
        <preserve_url>http://localhost:8080/IGRP/webapps?r=igrp/page/preserveUrl&amp;p_id=</preserve_url>
    </plsql>
    <content>
        <title>DashBoard</title>
        <form_1 type="form" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <tipo_objeto name="p_tipo_objeto" type="select" multiple="false" domain="" maxlength="30" required="false" change="false" disabled="false" right="false">
                    <label>Tipo Objeto</label>
                    <list>
                        <option/>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </tipo_objeto>
                <data_de name="p_data_de" type="date" format="IGRP_datePicker" maxlength="30" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false" class="default">
                    <label>Data de</label>
                    <value/>
                </data_de>
                <data_ate name="p_data_ate" type="date" format="IGRP_datePicker" maxlength="30" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false" class="default">
                    <label>Data ate</label>
                    <value/>
                </data_ate>
            </fields>
            <tools-bar>
                <item type="form" code="" class="primary" rel="pesquisar">
                    <title>Pesquisar</title>
                    <app>kofax</app>
                    <page>DashBoard</page>
                    <link>index</link>
                    <parameter>undefinedimages/IGRP/IGRP2.3/app/kofax/dashboard/DashBoard.xsl?</parameter>
                    <target>submit</target>
                    <img>primary|fa-search</img>
                </item>
            </tools-bar>
        </form_1>
        <statbox_1 type="statbox" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <statbox_1_tit name="p_statbox_1_tit" type="text" maxlength="4000" persist="true">
                    <label>Box Title</label>
                    <value>Box Title</value>
                </statbox_1_tit>
                <statbox_1_val name="p_statbox_1_val" type="text" maxlength="4000" persist="true">
                    <label>Value</label>
                    <value>895</value>
                </statbox_1_val>
                <statbox_1_txt name="p_statbox_1_txt" type="text" maxlength="4000" persist="true">
                    <label>Url Text</label>
                    <value>Read More...</value>
                </statbox_1_txt>
                <statbox_1_url name="p_statbox_1_url" type="text" maxlength="4000" persist="true">
                    <label>Url</label>
                    <value>http://www.example.com</value>
                </statbox_1_url>
                <statbox_1_bg name="p_statbox_1_bg" type="text" maxlength="4000" persist="true">
                    <label>Background</label>
                    <value>cp-emerald</value>
                </statbox_1_bg>
                <statbox_1_icn name="p_statbox_1_icn" type="text" maxlength="4000" persist="true">
                    <label>Icon</label>
                    <value>fa-check</value>
                </statbox_1_icn>
            </fields>
        </statbox_1>
        <chart_1 type="chart" xml-type="chart" title="Chart" gen-type="container" gen-group="">
            <caption/>
            <chart_type>column</chart_type>
            <xaxys>Eixo de X</xaxys>
            <yaxys>Eixo de Y</yaxys>
            <url>#</url>
            <label>
                <col>Ano</col>
                <col>X1</col>
                <col>X2</col>
                <col>X3</col>
                <col>X4</col>
            </label>
            <value>
                <row>
                    <col>2010</col>
                    <col>265</col>
                    <col>658</col>
                    <col>498</col>
                    <col>698</col>
                </row>
                <row>
                    <col>2009</col>
                    <col>784</col>
                    <col>258</col>
                    <col>594</col>
                    <col>498</col>
                </row>
                <row>
                    <col>2015</col>
                    <col>1010</col>
                    <col>698</col>
                    <col>366</col>
                    <col>498</col>
                </row>
            </value>
            <colors>
                <col>#ec99c0</col>
                <col>#7bd22a</col>
                <col>#beee69</col>
                <col>#a54d10</col>
            </colors>
        </chart_1>
    </content>
</rows>
