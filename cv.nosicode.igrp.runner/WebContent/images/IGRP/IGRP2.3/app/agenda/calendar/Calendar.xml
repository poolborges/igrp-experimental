<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="images/IGRP/IGRP2.3/app/agenda/calendar/Calendar.xsl" type="text/xsl"?>
<rows>
    <site>
        <welcome_note>Ola</welcome_note>
        <footer_note>2011 - Copyright NOSI</footer_note>
        <user_name>red-igrp</user_name>
        <button type="specific">
            <title>Sair</title>
            <app>RED</app>
            <page />
            <link>#</link>
            <target>_self</target>
            <img>exit.png</img>
        </button>
    </site>
    <page>Calendar</page>
    <app>agenda</app>
    <plsql>
        <action>index</action>
        <package_instance></package_instance>
        <package_db>nosi.webapps.agenda.pages</package_db>
        <package_html>Calendar</package_html>
        <table_name></table_name>
        <with_replace>false</with_replace>
        <with_label>false</with_label>
        <with_biztalk>false</with_biztalk>
        <dynamic_menu>false</dynamic_menu>
        <copy_menu>false</copy_menu>
        <package_copy_db />
        <package_copy_html />
        <preserve_url>http://localhost:8080/IGRP/webapps?r=igrp/page/preserveUrl&amp;p_id=</preserve_url>
    </plsql>
    <content>
        <title>Calendar</title>
        <calendar_1 type="calendar" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <calendar_1_lang name="p_calendar_1_lang" type="text" maxlength="30">
                    <label>Linguagem</label>
                    <value>pt</value>
                </calendar_1_lang>
                <calendar_1_event name="p_calendar_1_event" type="link" maxlength="4000">
                    <label>Load Eventos</label>
                    <value>/IGRP/images/IGRP/IGRP2.3/core/calendar/3.0.1/data/calendar.json</value>
                </calendar_1_event>
                <calendar_1_add name="p_calendar_1_add" type="link" maxlength="4000">
                    <label>add Eventos</label>
                    <value/>
                </calendar_1_add>
                <calendar_1_view name="p_calendar_1_view" type="text" maxlength="30">
                    <label>Default View</label>
                    <value/>
                </calendar_1_view>
                <calendar_1_date name="p_calendar_1_date" type="date" maxlength="30">
                    <label>Default View</label>
                    <value/>
                </calendar_1_date>
            </fields>
        </calendar_1>
    </content>
</rows>
